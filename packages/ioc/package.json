	"description": "A powerful IoC container for TypeScript applications",
	"author": "Isqanderm <aleksandr.melnik.personal@gmail.com> (https://www.linkedin.com/in/isqander-melnik)",
	"homepage": "https://github.com/Isqanderm/ioc#readme",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/Isqanderm/ioc.git"
	},
	"bugs": {
		"url": "https://github.com/Isqanderm/ioc/issues"
	},
	"keywords": [
		"IoC",
		"dependency injection",
		"inversion control",
		"typescript",
		"DI",
		"framework",
		"container"
	],
	"files": [
		"dist",
		"package.json",
		"LICENSE",
		"README.md"
	],
	"main": "./dist/node/server.js",
	"browser": "./dist/browser/browser.js",
	"module": "./dist/esm/index.js",
	"types": "./dist/esm/index.d.ts",
	"license": "MIT",
	"peerDependencies": {
		"reflect-metadata": "^0.1.12 || ^0.2.0"
	},
	"devDependencies": {
		"@nexus-ioc/testing": "^0.4.0",
		"@types/jest": "^29.5.12",
		"@types/node": "^22.5.4",
		"benchmark": "^2.1.4",
		"jest": "^29.7.0",
		"reflect-metadata": "^0.1.12 || ^0.2.0",
		"sinon": "^18.0.0",
		"ts-jest": "^29.1.4",
		"ts-node": "^10.9.2",
		"typescript": "^5.5.3"
	},
	"dependencies": {
		"tslib": "^2.6.3"
	},
	"scripts": {
		"build:node": "tsc -p tsconfig.node.json",
		"build:browser": "tsc -p tsconfig.browser.json",
		"build:esm": "tsc -p tsconfig.esm.json",
		"build": "npm run build:node && npm run build:browser && npm run build:esm",
		"test": "jest",
		"clean": "rm -rf ./dist && rm tsconfig.tsbuildinfo",
		"test:watch": "jest --watch",
		"code:format": "npx @biomejs/biome format --write",
		"code:lint": "npx @biomejs/biome lint --write",
		"code:check": "npx @biomejs/biome check",
		"code:check:ci": "npx @biomejs/biome ci",
		"code:check:apply": "npx @biomejs/biome check --write",
		"test:ready": "echo 0"
	}
}
